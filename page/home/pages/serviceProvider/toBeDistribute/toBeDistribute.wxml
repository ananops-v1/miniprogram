<view class="page">
  <view class="page__bd">
    <!-- bindscrolltolower="lower"  scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}" -->
    <scroll-view scroll-y="true" class="container discovery">
      <mp-cells>
        <mp-cell wx:for="{{orderList}}" wx:for-item="order" wx:key="order" data-id="{{order.mdmcTask.id}}" data-projectid="{{order.mdmcTask.projectId}}" bindtap="clickOrder">
          <view slot="title" class="mp-cell-title">
            <image src="../../../../../imgs/icon/to_be_confirmed.png" class="mp-cell-image" />
          </view>
          <view>项目:{{order.pmcProjectDto.contractName}}</view>
          <view class="mp-cell-view">任务名称:{{order.mdmcTask.title}}</view>
          <view class="mp-cell-view">报修时间:{{order.mdmcTask.appointTime}}</view>
          <view wx:if="{{order.mdmcTask.status!=14}}" class="operation_view">
            <!-- <view class="operation_button" catchtap="receiveOrder" data-id="{{order.mdmcTask.id}}" data-status="{{order.mdmcTask.status}}" data-projectid="{{order.mdmcTask.projectId}}">派单</view> -->
            <!-- <view class="operation_button" catchtap="rejectOrder" data-id="{{order.id}}" data-status="{{order.status}}">拒绝</view> -->
          </view>
        </mp-cell>

      </mp-cells>
    </scroll-view>
  </view>
</view>